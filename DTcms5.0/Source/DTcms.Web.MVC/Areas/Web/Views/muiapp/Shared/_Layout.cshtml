@using DTcms.Model;
@using DTcms.Common;
@{
   DTcms.Model.sites site = ViewData["site"] as DTcms.Model.sites;
   DTcms.Model.sysconfig config = ViewData["sysConfig"] as DTcms.Model.sysconfig;
   DTcms.Web.MVC.UI.Controllers.BaseController c = ViewBag.BasePage as DTcms.Web.MVC.UI.Controllers.BaseController;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <title>@ViewBag.Title</title>
    <meta name="keywords" content="@site.seo_keyword" />
    <meta name="discription" content="@site.seo_description" />
    <link href="~/templates/@site.templet_path/iconfont/iconfont.css" rel="stylesheet">
    <link href="~/templates/@site.templet_path/css/mui.min.css" rel="stylesheet">
    <link href="~/templates/@site.templet_path/css/all.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/templates/@site.templet_path/css/base.css" />
    <link rel="stylesheet" type="text/css" href="~/templates/@site.templet_path/css/style.css" />
    <script src="~/templates/@site.templet_path/js/jquery-1.8.3.min"></script>
    <script src="~/templates/@site.templet_path/js/common.js"></script>
    @RenderSection("HeaderContent", false)
    @RenderSection("Scripts_Head", false)
</head>
<body>
    @RenderBody()
</body>
    @RenderSection("Scripts_Foot", false)
</html>