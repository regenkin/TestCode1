@using System.Data;
@using DTcms.Web.MVC.Areas.Web.Controllers;
@using DTcms.Web.MVC.UI.Controllers;
@{
   Layout = "~/Areas/Web/Views/" + ViewBag.TemplateSkin + "/web/Shared/_LayoutList.cshtml";
   DTcms.Model.site_channel site = ViewData["site"] as DTcms.Model.site_channel;
   DTcms.Model.siteconfig config = ViewData["config"] as DTcms.Model.siteconfig;
   ViewBag.Title = site.seo_title;
   IndexController c = ViewBag.Controller as IndexController;
   Response.Redirect(c.linkurl("goods")+"?site=shop");
}
@section HeaderContent{
   <link href="~/templates/@ViewBag.TemplateSkin/web/css/style.css" rel="stylesheet" />
   <script src="~/Scripts/jquery/jquery.flexslider-min.js"></script>
}
@section Scripts{
   <script>
      $(function () {
         window.location.href = "@c.linkurl("goods")?site=shop";
      });
   </script>
}
