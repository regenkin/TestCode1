@{
    ViewBag.Title = "RoleFieldPermissionSet";
}
@using Kinfar.Frame.Common;
@using Kinfar.Frame.UIOperate;
@{ UIFrameFactory frameFactory = UIFrameFactory.GetInstance();
   string moduleName = Server.UrlDecode(Request["moduleName"].ObjToStr());
   string roleName = Server.UrlDecode(Request["roleName"].ObjToStr());
   int type = Request["type"].ObjToInt();}
@Html.Raw(frameFactory.GetRoleFieldPermissionSetHTML(moduleName, roleName, type))
