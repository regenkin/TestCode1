@{
    ViewBag.Title = "QuickEditForm";
}
@using Kinfar.Frame.Common;
@using Kinfar.Frame.UIOperate;
@{ UIFrameFactory frameFactory = UIFrameFactory.GetInstance();
   Guid moduleId = UIOperate.GetModuleIdByRequest(Request);
   Guid roleId = Request["roleId"].ObjToGuid();
   Guid? formId = Request["formId"].ObjToGuidNull();}
@Html.Raw(frameFactory.GetQuickEditFormHTML(moduleId, roleId, formId))
